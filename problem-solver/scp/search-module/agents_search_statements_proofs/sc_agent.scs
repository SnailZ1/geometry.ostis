sc_agent_of_statements_proofs_searching
=> nrel_main_idtf:
	[sc-агент поиска доказательства утверждений] (* <- lang_ru;; *);
	[sc-agent of statements proofs searching] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_after_generate_outgoing_arc => action_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_condition_state => ..sc_agent_result_state);

=> nrel_sc_agent_action_class:
	action_find_statements_proofs;

<= nrel_sc_agent_key_sc_elements: 
	{
	action_initiated;
	action;
	action_find_statements_proofs
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_statements_proofs_searching
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_proc_of_statements_proofs_searching
		};;

	-> sc_agent_of_statements_proofs_searching_scp
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_condition_state
= [*
	action_find_statements_proofs _-> .._action;;
	action_initiated _-> .._action;;
	action _-> .._action;;
	.._action _-> .._parameter;;
*];;

..sc_agent_result_state
= [*
	action_find_statements_proofs _-> .._action;;
	action_finished _-> .._action;;
	action _-> .._action;;
	.._action _=> nrel_result:: .._answer;;
	.._action _-> .._parameter;;
*];;

action_find_statements_proofs
<- sc_node_class;
<= nrel_inclusion: information_action;;
